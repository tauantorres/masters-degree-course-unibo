(set-logic QF_LRA)
(set-option :produce-models true)
(declare-fun x () Real)
(declare-fun y () Real)
(declare-fun A1 () Bool)
(declare-fun A2 () Bool)
(assert (or (not A1) (>= (+ (* 2 x) y) (- 2))))
(assert (or A1 (>= (+ x y) 3)))
(assert (or (not A2) (>= (- (* 4 x) y) (- 4))))
(assert (or A2 (>= (- (* 2 x) y) (- 6))))
(check-sat)
